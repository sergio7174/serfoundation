<h2>Users Table</h2>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Password</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
    <tr>
      <td>
        <a href="<%= `/users/${user._id}` %>">
          <%= user.name %>
        </a>
      </td>
      <td>
        <%= user.username %>
      </td>
      <td>
        <%= user.password %>
      </td>
      <td>
       <button> <a href="<%=`/users/${user._id}/edit`%>"> 
          Edit
        </a> </button>
      </td>
      <td>
        <!-- Recall that you need to use the _method=DELETE query parameter so that your application
             can interpret GET requests as DELETE requests-->
        <a href="<%=`/users/${user._id}/delete?_method=DELETE` %>" onclick="return confirm('Are you sure you want to delete this record?')">Delete</a>
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>